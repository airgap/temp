{
	"name": "@lyku/mark-notifications-read",
	"extends": ["../_shared/project.json"],
	"tags": [],
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"outputs": ["{options.outputPath}"],
			"options": {
				"command": "./scripts/build-route mark-notifications-read"
			}
		},
		"dockerize": {
			"executor": "nx:run-commands",
			"options": {
				"command": "./scripts/dockerize-route mark-notifications-read --push"
			}
		},
		"kubernetize": {
			"executor": "nx:run-commands",
			"options": {
				"command": "./scripts/deploy-route mark-notifications-read"
			}
		},
		"grafanize": {
			"executor": "nx:run-commands",
			"options": {
				"command": "./scripts/graph-route mark-notifications-read",
				"cwd": "{workspaceRoot}"
			}
		},
		"grafanize:wait": {
			"executor": "nx:run-commands",
			"options": {
				"command": "./scripts/graph-route mark-notifications-read --wait-for-metrics",
				"cwd": "{workspaceRoot}"
			}
		}
	}
}

	"scripts": {
		"build": "nx run-many --target=build --skip-nx-cache --parallel=99",
		"lint": "nx run-many --target=lint --skip-nx-cache --parallel=99",
		"test": "nx run-many --target=test --skip-nx-cache --parallel=99",
		"dockerize": "nx run-many --target=dockerize --skip-nx-cache --parallel=12",
		"kubernetize": "nx run-many --target=kubernetize --skip-nx-cache --parallel=12",
		"format": "prettier -w .",
		"clean-artifacts": "rimraf -g dist tmp .nx/cache",
		"clean-modules": "rimraf -g ./**/node_modules",
		"clean": "npm run clean-artifacts && npm run clean-modules",
		"generate-ingress": "bun scripts/generate-ingress.ts",
		"apply-ingress": "kubectl apply -f tmp/ingress.yaml",
		"deploy-ingress": "bun run generate-ingress && bun run apply-ingress"
	},

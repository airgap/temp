{
	"name": "stats",
	"$schema": "../../node_modules/nx/schemas/project-schema.json",
	"sourceRoot": "libs/stats/src",
	"projectType": "library",
	"targets": {
		"build": {
			"executor": "nx:run-commands",
			"options": {
				"commands": [
					"rimraf dist/libs/stats",
					"mkdirp dist/libs/stats",
					"bunx cloc --exclude-list=cloc-exclude --not-match-f=package-lock.json --vcs git --json . >dist/libs/stats/cloc.json",
					"node -e \"require('fs').writeFileSync('dist/libs/stats/pipes.json', process.env.REACT_APP_PIPE || '420')\"",
					"bun libs/stats/count-components.ts",
					"git rev-list --count --all > dist/libs/stats/revisions.json",
					"bun libs/stats/buildRouteMap.ts"
				],
				"parallel": false
			}
		}
	},
	"tags": []
}

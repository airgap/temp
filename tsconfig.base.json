{
	"compileOnSave": false,
	"compilerOptions": {
		"strict": true,
		"rootDir": ".",
		"sourceMap": true,
		"declaration": true,
		"moduleResolution": "node",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"target": "es2020",
		"module": "esnext",
		"lib": ["es2020", "dom"],
		"skipLibCheck": true,
		"skipDefaultLibCheck": true,
		"baseUrl": ".",
		"resolveJsonModule": true,
		"esModuleInterop": true,
		"maxNodeModuleJsDepth": 100,
		"paths": {
			"@lyku/clickhouse-client": ["libs/clickhouse-client/src/index.ts"],
			"@lyku/db-config": ["libs/db-config/src/index.ts"],
			"@lyku/db-types": ["dist/libs/db-types/kysely.d.ts"],
			"@lyku/defaults": ["libs/defaults/src/index.ts"],
			"@lyku/elasticsearch-client": ["libs/elasticsearch-client/src/index.ts"],
			"@lyku/get-current-user": ["libs/routes/getCurrentUser/src/index.ts"],
			"@lyku/handles": ["dist/libs/handles"],
			"@lyku/helpers": ["libs/helpers/src/index.ts"],
			"@lyku/json-models": ["dist/libs/json-models/src/index.js"],
			"@lyku/json-models/*": ["dist/libs/json-models/src/*"],
			"@lyku/locker": ["libs/locker/src/index.ts"],
			"@lyku/logger": ["libs/logger/src/index.ts"],
			"@lyku/mapi-models": ["libs/mapi-models/src/index.ts"],
			"@lyku/mapi-types": ["dist/libs/mapi-types/index.d.ts"],
			"@lyku/metrics": ["libs/metrics/src/index.ts"],
			"@lyku/mux-client": ["libs/mux-client/src/index.ts"],
			"@lyku/nats-client": ["libs/nats-client/src/index.ts"],
			"@lyku/phrasebooks": ["libs/phrasebooks/src/index.ts"],
			"@lyku/postgres-client": ["libs/postgres-client/src/index.ts"],
			"@lyku/queue-system": ["libs/queue-system/src/index.ts"],
			"@lyku/redis-client": ["libs/redis-client/src/index.ts"],
			"@lyku/route-helpers": ["libs/route-helpers/src/index.ts"],
			"@lyku/si-bits": ["libs/si-bits/src/index.ts"],
			"@lyku/stats": ["dist/libs/stats"],
			"@lyku/stock-docs": ["libs/stock-docs/src/index.ts"],
			"@lyku/strings": ["libs/strings/src/index.ts"],
			"bson-models": ["libs/bson-models/src/index.ts"],
			"monolith-ts-api": ["libs/monolith-ts-api/src/index.ts"],
			"@lyku/ttf-ais": ["libs/ttf-ais/src/index.ts"]
		}
	},
	"exclude": ["node_modules", "tmp"]
}
